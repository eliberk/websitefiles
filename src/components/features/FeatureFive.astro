---
const featureOne = [
  {
    id: 1,
    tagline: "YOUR NEXT FINTECH STARTUP",
    title: "Innovative financial solutions for every situation",
    description:
      "Revolutionizing the financial landscape with cutting-edge technology and expertise",
    features: [
      { icon: "iconOne", description: "Clear data visibility" },
      { icon: "iconTwo", description: "Reduced external factors" },
      { icon: "iconThree", description: "enhanced stabilty" },
      { icon: "iconFour", description: "accelerated times" },
      { icon: "iconOne", description: "Clear data visibility" },
      { icon: "iconTwo", description: "Reduced external factors" },
      { icon: "iconThree", description: "enhanced stabilty" },
      { icon: "iconFour", description: "accelerated times" },
    ],
    image: "/dashboard.svg",
  },{
    id: 1,
    tagline: "YOUR NEXT FINTECH STARTUP",
    title: "Innovative financial solutions for every situation",
    description:
      "Revolutionizing the financial landscape with cutting-edge technology and expertise",
    features: [
      { icon: "iconOne", description: "Clear data visibility" },
      { icon: "iconTwo", description: "Reduced external factors" },
      { icon: "iconThree", description: "enhanced stabilty" },
      { icon: "iconFour", description: "accelerated times" },
      { icon: "iconOne", description: "Clear data visibility" },
      { icon: "iconTwo", description: "Reduced external factors" },
      { icon: "iconThree", description: "enhanced stabilty" },
      { icon: "iconFour", description: "accelerated times" },
    ],
    image: "/dashboard.svg",
  },
];
---
<section class="relative overflow-hidden">
  {
    featureOne.map((features, index) => (
        <div class="flex flex-col items-center pt-24 pb-6 md:flex-row">
          <div class="max-w-screen-xl px-8 md:pl-12 lg:pl-44 lg:pb-0">
            <div>
              <span class="font-mono text-xs font-medium tracking-tight text-blue-600 uppercase ">
                {features.tagline}
              </span>
              <h1 class="mt-8 text-4xl font-semibold text-balance tracking-tighter text-blue-950 ">
                {features.title}
              </h1>
              <p class="text-balance mt-4 text-base text-gray-700 lg:text-base ">
                {features.description}
              </p>
             <div class="lg:grid hidden grid-cols-1 gap-2 mt-12 text-sm font-medium">
                {features.features.map((feature) => (
                    <div
                      key={feature.icon}
                      class="inline-flex items-center gap-2 font-mono text-xs tracking-tighter uppercase"
                    >
                      {feature.icon === "iconOne" && (
                        <svg
                          class="icon icon-tabler text-blue-700 size-4 icon-tabler-360"
                          viewBox="0 0 24 24"
                          stroke-width="2"
                          stroke="currentColor"
                          fill="none"
                          stroke-linecap="round"
                          stroke-linejoin="round"
                        >
                          <path stroke="none" d="M0 0h24v24H0z" fill="none" />
                          <path d="M17 15.328c2.414 -.718 4 -1.94 4 -3.328c0 -2.21 -4.03 -4 -9 -4s-9 1.79 -9 4s4.03 4 9 4" />
                          <path d="M9 13l3 3l-3 3" />
                        </svg>
                      )}
                      {feature.icon === "iconTwo" && (
                        <svg
                          class="icon icon-tabler text-blue-700 size-4 icon-tabler-antenna-bars-3"
                          viewBox="0 0 24 24"
                          stroke-width="2"
                          stroke="currentColor"
                          fill="none"
                          stroke-linecap="round"
                          stroke-linejoin="round"
                        >
                          <path stroke="none" d="M0 0h24v24H0z" fill="none" />
                          <path d="M6 18l0 -3" />
                          <path d="M10 18l0 -6" />
                          <path d="M14 18l0 .01" />
                          <path d="M18 18l0 .01" />
                        </svg>
                      )}
                      {feature.icon === "iconThree" && (
                        <svg
                          class="icon icon-tabler text-blue-700 size-4 icon-tabler-load-balancer"
                          viewBox="0 0 24 24"
                          stroke-width="2"
                          stroke="currentColor"
                          fill="none"
                          stroke-linecap="round"
                          stroke-linejoin="round"
                        >
                          <path stroke="none" d="M0 0h24v24H0z" fill="none" />
                          <path d="M12 13m-3 0a3 3 0 1 0 6 0a3 3 0 1 0 -6 0" />
                          <path d="M12 20m-1 0a1 1 0 1 0 2 0a1 1 0 1 0 -2 0" />
                          <path d="M12 16v3" />
                          <path d="M12 10v-7" />
                          <path d="M9 6l3 -3l3 3" />
                          <path d="M12 10v-7" />
                          <path d="M9 6l3 -3l3 3" />
                          <path d="M14.894 12.227l6.11 -2.224" />
                          <path d="M17.159 8.21l3.845 1.793l-1.793 3.845" />
                          <path d="M9.101 12.214l-6.075 -2.211" />
                          <path d="M6.871 8.21l-3.845 1.793l1.793 3.845" />
                        </svg>
                      )}
                      {feature.icon === "iconFour" && (
                        <svg
                          class="icon icon-tabler text-blue-700 size-4 icon-tabler-brand-speedtest"
                          viewBox="0 0 24 24"
                          stroke-width="2"
                          stroke="currentColor"
                          fill="none"
                          stroke-linecap="round"
                          stroke-linejoin="round"
                        >
                          <path stroke="none" d="M0 0h24v24H0z" fill="none" />
                          <path d="M5.636 19.364a9 9 0 1 1 12.728 0" />
                          <path d="M16 9l-4 4" />
                        </svg>
                      )}
                      {feature.icon === "iconFive" && (
                        <svg
                          xmlns="http://www.w3.org/2000/svg"
                          class="icon icon-tabler icon-tabler-database text-blue-700 size-4"
                          viewBox="0 0 24 24"
                          stroke-width="1.5"
                          stroke="currentColor"
                          fill="none"
                          stroke-linecap="round"
                          stroke-linejoin="round"
                        >
                          <path stroke="none" d="M0 0h24v24H0z" fill="none" />
                          <path d="M12 6m-8 0a8 3 0 1 0 16 0a8 3 0 1 0 -16 0" />
                          <path d="M4 6v6a8 3 0 0 0 16 0v-6" />
                          <path d="M4 12v6a8 3 0 0 0 16 0v-6" />
                        </svg>
                      )}
                      {feature.icon === "iconSix" && (
                        <svg
                          xmlns="http://www.w3.org/2000/svg"
                          class="icon icon-tabler icon-tabler-building text-blue-700 size-4"
                          viewBox="0 0 24 24"
                          stroke-width="1.5"
                          stroke="currentColor"
                          fill="none"
                          stroke-linecap="round"
                          stroke-linejoin="round"
                        >
                          <path stroke="none" d="M0 0h24v24H0z" fill="none" />
                          <path d="M3 21l18 0" />
                          <path d="M9 8l1 0" />
                          <path d="M9 12l1 0" />
                          <path d="M9 16l1 0" />
                          <path d="M14 8l1 0" />
                          <path d="M14 12l1 0" />
                          <path d="M14 16l1 0" />
                          <path d="M5 21v-16a2 2 0 0 1 2 -2h10a2 2 0 0 1 2 2v16" />
                        </svg>
                      )}
                      {feature.icon === "iconSeven" && (
                        <svg
                          xmlns="http://www.w3.org/2000/svg"
                          class="icon icon-tabler icon-tabler-augmented-reality-2 text-blue-700 size-4"
                          viewBox="0 0 24 24"
                          stroke-width="1.5"
                          stroke="currentColor"
                          fill="none"
                          stroke-linecap="round"
                          stroke-linejoin="round"
                        >
                          <path stroke="none" d="M0 0h24v24H0z" fill="none" />
                          <path d="M10 21h-2a2 2 0 0 1 -2 -2v-14a2 2 0 0 1 2 -2h8a2 2 0 0 1 2 2v3.5" />
                          <path d="M17 17l-4 -2.5l4 -2.5l4 2.5v4.5l-4 2.5z" />
                          <path d="M13 14.5v4.5l4 2.5" />
                          <path d="M17 17l4 -2.5" />
                          <path d="M11 4h2" />
                        </svg>
                      )}
                      {feature.icon === "iconEight" && (
                        <svg
                          xmlns="http://www.w3.org/2000/svg"
                          class="icon icon-tabler icon-tabler-time-duration-0 text-blue-700 size-4"
                          viewBox="0 0 24 24"
                          stroke-width="1.5"
                          stroke="currentColor"
                          fill="none"
                          stroke-linecap="round"
                          stroke-linejoin="round"
                        >
                          <path stroke="none" d="M0 0h24v24H0z" fill="none" />
                          <path d="M3 12v.01" />
                          <path d="M21 12v.01" />
                          <path d="M12 21v.01" />
                          <path d="M12 3v.01" />
                          <path d="M7.5 4.2v.01" />
                          <path d="M16.5 4.2v.01" />
                          <path d="M16.5 19.8v.01" />
                          <path d="M7.5 19.8v.01" />
                          <path d="M4.2 16.5v.01" />
                          <path d="M19.8 16.5v.01" />
                          <path d="M19.8 7.5v.01" />
                          <path d="M4.2 7.5v.01" />
                          <path d="M10 11v2a2 2 0 1 0 4 0v-2a2 2 0 1 0 -4 0z" />
                        </svg>
                      )}
                      <span class="text-sm font-medium">{feature.description}</span>
                    </div>
                 ))}
             </div>
            </div>
          </div>

          <div  class={`h-full ${index % 2 === 0 ? "mr-auto -ml-48 p-8 lg:p-0 md:order-first" : ""}`}>
            <div class="overflow-hidden bg-white rounded-3xl lg:h-full shadow-box shadow-gray-500/20">
              <div class="overflow-y-hidden lg:h-auto">
                <div class="lg:h-auto">
                  <img alt="#" src="/dashboard.svg" />
                </div>
              </div>
            </div>
          </div>
        </div>
    ))
  }


</section>
